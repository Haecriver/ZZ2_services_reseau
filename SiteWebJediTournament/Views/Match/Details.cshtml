@model SiteWebJediTournament.Models.MatchModels

@{
    ViewBag.Title = "Details d'un match";
}

<h2>Détails sur un match</h2>

<div>
    
    <hr />
    <h4>Propriétés</h4>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Jedi1.Default.Nom)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Jedi1.Jedi.Nom)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Jedi2.Default.Nom)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Jedi2.Jedi.Nom)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.PhaseTournoi)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.PhaseTournoi)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Stade.Default.Planete)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Stade.Stade.Planete)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Modifier", "Edit", new { id = Model.Id }, htmlAttributes: new { @class = "btn btn-warning" })
    @Html.ActionLink("Retour à la liste", "Index", null, htmlAttributes: new { @class = "btn btn-primary" })
</p>
